    "url": "git+https://github.com/Humberd/ng-boost.git"
  },
  "bugs": {
    "url": "https://github.com/Humberd/ng-boost/issues"
  },
  "homepage": "https://github.com/Humberd/ng-boost#readme",
  "peerDependencies": {
    "@angular/common": "^8.0.1",
    "@angular/core": "^8.0.1",
    "@angular/router": "^8.0.1",
    "@angular/forms": "^8.0.1",
    "rxjs": "^6.5.2"
  }
}
