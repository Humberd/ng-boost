{
  "name": "ng-boost",
  "author": "Maciej Sawicki",
  "homepage": "https://humberd.github.io/ng-boost/",
  "version": "0.0.1-beta.1",
  "license": "MIT",
  "description": "Angular library, which boosts up a development by enforcing consistent patterns.",
  "keywords": [
    "Angular",
    "framework",
    "library",
    "patterns",
    "boost"
  ],
  "scripts": {
    "build-core": "ng build core --prod",
    "build-core:watch": "ng build core --prod --watch",
    "build-core:ci": "ng build core --prod",
    "build-material": "ng build material --prod",
    "build-material:watch": "ng build material --prod --watch",
    "build-material:ci": "ng build material --prod",
    "version:beta-core": "cd projects/core && npm run version:beta",
    "test-core:ci": "ng test core -c ci --code-coverage",
    "test-material:ci": "ng test material -c ci --code-coverage",
    "publish-ng-boost": "cd projects/core && npm run publish-lib",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "bump-version:beta": "node scripts/version-update.js",
    "release": "node scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Humberd/ng-boost.git"
  },
  "bugs": {
    "url": "https://github.com/Humberd/ng-boost/issues"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@babel/core": "^7.8.3",
    "@types/jasmine": "~3.5.1",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.11.0",
    "@types/shelljs": "^0.8.6",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.1",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^9.1.0",
    "protractor": "~5.4.2",
    "puppeteer": "^2.0.0",
    "shelljs": "^0.8.3",
    "ts-node": "~8.6.2",
    "tslint": "~5.20.1",
    "typescript": "~3.7.5"
  }
}
